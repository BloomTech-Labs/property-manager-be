  "homepage": "https://github.com/Lambda-School-Labs/property-manager-be#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jest": "^24.9.0",
    "knex": "^0.20.1",
    "knex-cleaner": "^1.3.0",
    "nodemon": "^1.19.4",
    "pg": "^7.12.1",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
